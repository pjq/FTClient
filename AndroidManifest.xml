<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.impjq.ftclient" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".FTClient" android:label="@string/app_name"
			android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".UpdateStatusActivity"
			android:configChanges="orientation|keyboardHidden"></activity>
		<activity android:name="net.impjq.photo.CapturePhoto"
			android:configChanges="orientation|keyboardHidden"></activity>
		<activity android:name="net.impjq.ftclient.RegisterActivity"
			android:configChanges="orientation|keyboardHidden" android:theme="@android:style/Theme.Dialog"></activity>
	</application>

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.CAMERA"></uses-permission>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
</manifest> 